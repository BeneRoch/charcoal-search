{{!--
`charcoal/admin/widget/search/no-results-search`
--}}

{{#hasNoResultsSearches}}
<div class="o-section">
    Recherches sans résultats

    <table class="table table-bordered table-hover">
        <thead>
        <tr>
            <th>Mot-clé</th>
            <th>Nombre de recherches</th>
            <th>Nombre de résultats</th>
        </tr>
        </thead>
        <tbody>
        {{#noResultsSearches}}
        <tr>
            <td>{{keyword}}</td>
            <td>{{num_searches}}</td>
            <td>{{num_results}}</td>
        </tr>
        {{/noResultsSearches}}
        </tbody>
    </table>
</div>
{{/hasNoResultsSearches}}
